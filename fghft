local function ZKXWNX_fake_script()
	local script = Instance.new('LocalScript', attack1)

	local args = {
		[1] = game:GetService("Players").LocalPlayer.PlayerGui.Attacks.Frame.Attack1.Info
	}
	
	local actived = false
	
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		if actived == false then
			actived = true
			script.Parent.Parent.attack1status.Text = "РАБОТАЕТ КАК ШВЕЙЦАРСКИЕ ЧАСЫ"
		else
			actived = false
			script.Parent.Parent.attack1status.Text = "ВЫКЛЮЧЕНО"
		end
	end)
	
	while task.wait() do
		if actived == true then
			game:GetService("ReplicatedStorage").UseMove:FireServer(unpack(args))
		else
			print("waiting")
		end
	end
end
coroutine.wrap(ZKXWNX_fake_script)()
